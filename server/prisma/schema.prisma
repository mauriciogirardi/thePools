generator client {
  provider = "prisma-client-js"
}

generator erd {
  provider = "prisma-erd-generator"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Pool {
  id        String   @id @default(cuid())
  code      String   @unique
  title     String
  createdAt DateTime @default(now())
}
